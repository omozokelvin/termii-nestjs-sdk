{
  "name": "termii-nestjs",
  "version": "1.1.0",
  "description": "A NestJS SDK for Termii API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "nestjs",
    "termii",
    "sdk"
  ],
  "author": "Kelvin Omozokpia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omozokelvin/termii-nestjs-sdk.git"
  },
  "homepage": "https://github.com/omozokelvin/termii-nestjs-sdk#readme",
  "bugs": {
    "url": "https://github.com/omozokelvin/termii-nestjs-sdk/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@types/node": "^20.8.9",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "axios": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  }
}
